		숫자3 : <input type="text" name="no3" id="no3"><br>
		숫자4 : <input type="text" name="no4" id="no4"><br>
		숫자5 : <input type="text" name="no5" id="no5"><br>
		숫자6 : <input type="text" name="no6" id="no6"><br>
		<input type="button" value="전송" onclick="efg()">
	</form>
<script>
		function efg(){
		if (f.no1.value == ""){
			alert("첫번째 숫자 값을 입력하세요.");
			no1.focus();
		}
		else if(f.no2.value == ""){
			alert("두번째 숫자 값을 입력하세요.");
			no2.focus();
		}
		else if(f.no3.value == ""){
			alert("세번째 숫자 값을 입력하세요.");
			no3.focus();
		}
		else if(f.no4.value == ""){
			alert("네번째 숫자 값을 입력하세요.");
			no4.focus();
		}
		else if(f.no5.value == ""){
			alert("다섯번째 숫자 값을 입력하세요.");
			no5.focus();
		}
		else if(f.no6.value == ""){
			alert("여섯번째 숫자 값을 입력하세요.");
			no6.focus();
		}
		else{
			f.submit();
		}
	}
</script>

